  <!-- ---------- SCRIPTS: MODO OSCURO + PEQUE√ëAS INTERACCIONES ---------- -->
  <script>
    // Inserta el a√±o en el footer
    document.getElementById('year').textContent = new Date().getFullYear();

    // Dark mode toggle with localStorage persistence
    const toggle = document.getElementById('theme-toggle');
    const root = document.documentElement;
    // Aplicar preferencia inicial (localStorage -> prefers-color-scheme)
    const saved = localStorage.getItem('nt_theme'); // 'dark' or 'light'
    const prefersDark = window.matchMedia && window.matchMedia('(prefers-color-scheme: dark)').matches;
    function applyTheme(mode){
      if(mode === 'dark'){
        document.body.classList.add('dark');
        toggle.textContent = '‚òÄÔ∏è Claro';
        toggle.setAttribute('aria-pressed','true');
      } else {
        document.body.classList.remove('dark');
        toggle.textContent = 'üåô Oscuro';
        toggle.setAttribute('aria-pressed','false');
      }
    }
    // iniciar
    if(saved === 'dark' || (!saved && prefersDark)){
      applyTheme('dark');
    } else {
      applyTheme('light');
    }

    toggle.addEventListener('click', ()=>{
      const isDark = document.body.classList.toggle('dark');
      applyTheme(isDark ? 'dark' : 'light');
      localStorage.setItem('nt_theme', isDark ? 'dark' : 'light');
    });

    // Mejora: abrir links internos con offset si header sticky (si se desea)
    // (no es estrictamente necesario, pero mejora la UX)
    document.querySelectorAll('a[href^="#"]').forEach(el=>{
      el.addEventListener('click', (e)=>{
        const href = el.getAttribute('href');
        if(href.length > 1){
          e.preventDefault();
          const target = document.querySelector(href);
          if(target){
            const y = target.getBoundingClientRect().top + window.scrollY - 72; // offset nav
            window.scrollTo({top: y, behavior: 'smooth'});
          }
        }
      });
    });
  </script>
